<% au = AuthUser.find_by_id(session[:auth_user_id]) %>
<% if au %>

<pre>
id: <%= au.id %>
provider: <%= au.provider %>
uid: <%= au.uid %>
name: <%= au.name %>
screen_name: <%= au.screen_name %>
nickname: <%= au.nickname %>
img_url:  <%= au.img_url %>
<img src="<%= au.img_url %>" />
credentials: <%= au.credentials %>
token: <%= au.token %>
secret: <%= au.secret %>
user_info: <%= au.user_info %>
</pre>


<!-- ログアウトリンクつけたよ -->
<br><%= link_to 'ログアウト', '/auth/twitter/signout' %>



<!-- 特定ユーザーのツイート取ってくるンゴ clintにログインしたユーザーの情報が入ってる。→ApplicatonController.rb twitter_clientで明記 -->
<br>

<%= @client.home_timeline.first.id %>
<br>
<% @client.home_timeline({count: 5}).each do |tweet| %>
<%= tweet.full_text %>
<br>
<% end %>


<% else %>

認証ログインしてないぞい！
<%= link_to 'ログイン', '/auth/twitter' %>

<% end %>